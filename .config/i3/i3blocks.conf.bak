separator=true
markup=pango
align=center

# [mediaplayer]
# command=~/.config/i3/scripts/mediaplayer
# interval=2
# signal=10

[usb]
command=~/.config/i3/scripts/usb
markup=pango
signal=1
interval=10
IGNORE_LIST=[]
MOUNTED_COLOR=gray
PLUGGED_COLOR=gray

[cpu]
command=~/.config/i3/scripts/cpu_usage
label=CPU 
interval=2

[memory]
command=~/.config/i3/scripts/memory
label=MEM 
interval=2

[time]
command=date '+%A, %b %d %H:%M:%S'
interval=1
